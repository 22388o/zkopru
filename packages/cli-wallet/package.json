{
  "name": "@zkopru/cli-wallet",
  "version": "0.0.4",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "~zk-wizard": "dist"
  },
  "bin": {
    "zkopru-wallet": "dist/cli.js"
  },
  "keywords": [
    "cli"
  ],
  "preferGlobal": true,
  "scripts": {
    "prebuild": "shx mkdir -p dist",
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build tsconfig.build.json --clean && shx rm -rf coverage *.log junit.xml dist && jest --clearCache",
    "link-modules": "link-module-alias",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --reporters='jest-junit'",
    "coverage": "jest --coverage",
    "coverage:show": "live-server coverage",
    "dev": "ts-node src/cli.ts -f --ws ws://localhost:5000 --coordinator http://localhost:8888",
    "dev:config": "ts-node src/cli.ts --config wallet.json",
    "start": "node dist/cli.js"
  },
  "dependencies": {
    "@zkopru/account": "file:../account",
    "@zkopru/babyjubjub": "file:../babyjubjub",
    "@zkopru/contracts": "file:../contracts",
    "@zkopru/core": "file:../core",
    "@zkopru/prisma": "file:../prisma",
    "@zkopru/transaction": "file:../transaction",
    "@zkopru/tree": "file:../tree",
    "@zkopru/utils": "file:../utils",
    "@zkopru/zk-wizard": "file:../zk-wizard",
    "big-integer": "^1.6.48",
    "bip39": "^3.0.2",
    "blessed": "^0.1.81",
    "bn.js": "^5.1.1",
    "chalk": "^4.0.0",
    "cli-progress": "^3.8.2",
    "figlet": "^1.4.0",
    "follow-redirects": "^1.11.0",
    "fs-extra": "^9.0.0",
    "node-ansiparser": "^2.2.0",
    "node-ansiterminal": "^0.2.1-beta",
    "node-fetch": "^2.6.0",
    "prompts": "^2.3.2",
    "tar": "^6.0.2",
    "web3": "^1.2.6",
    "web3-core": "^1.2.6",
    "web3-eth-contract": "^1.2.6",
    "web3-utils": "^1.2.6",
    "yargs": "^15.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
