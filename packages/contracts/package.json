{
  "name": "@zkopru/contracts",
  "version": "0.0.1",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "~contracts": "dist"
  },
  "dependencies": {
    "bn.js": "^5.1.1",
    "web3": "^1.2.6",
    "web3-core": "^1.2.6",
    "web3-eth-contract": "^1.2.6"
  },
  "scripts": {
    "precommit": "#lint-staged",
    "compile": "truffle compile",
    "postinstall": "shx mkdir -p dist",
    "build:": "yarn compile && yarn genTS && tsc --build tsconfig.build.json && yarn prettier --write src/**/*",
    "clean": "tsc --build tsconfig.build.json --clean && shx rm -rf coverage *.log junit.xml dist && jest --clearCache",
    "migrate:test": "truffle migrate --network test && yarn genTS",
    "link-modules": "link-module-alias",
    "tsc": "tsc --build tsconfig.build.json && link-module-alias",
    "test": "truffle test",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "web3TS": "typechain --target web3-v1 './build/**/*.json' --outDir src/contracts",
    "genABI": "node utils/abi-generator.js",
    "genTS": "yarn web3TS && yarn genABI && node utils/ts-generator.js",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@truffle/artifactor": "^4.0.54",
    "merkle-tree-rollup": "^1.1.4",
    "smt-rollup": "^0.6.4",
    "solc5": "npm:solc@0.5.15",
    "circomlib": "^0.1.2"
  }
}
