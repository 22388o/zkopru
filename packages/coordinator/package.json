{
  "name": "@zkopru/coordinator",
  "version": "0.0.1",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "zkopru-coordinator": "dist/cli/index.js"
  },
  "keywords": [
    "zkopru, coordinator, cli, verifier"
  ],
  "_moduleAliases": {
    "~account": "../account/dist",
    "~coordinator": "dist",
    "~database": "../database/dist",
    "~utils": "../utils/dist"
  },
  "scripts": {
    "postinstall": "shx mkdir -p dist",
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build tsconfig.build.json --clean && shx rm -rf coverage *.log junit.xml dist && jest --clearCache",
    "link-modules": "link-module-alias",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --reporters='jest-junit'",
    "coverage": "jest --coverage",
    "coverage:show": "live-server coverage",
    "dev": "ts-node src/cli/index.ts --ws ws://localhost:5000 --config coordinator.json",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@nano-sql/adapter-leveldb": "^2.0.5",
    "@nano-sql/core": "^2.3.7",
    "@zkopru/babyjubjub": "file:../babyjubjub",
    "@zkopru/contracts": "file:../contracts",
    "@zkopru/core": "file:../core",
    "@zkopru/transaction": "file:../transaction",
    "@zkopru/tree": "file:../tree",
    "@zkopru/utils": "file:../utils",
    "big-integer": "^1.6.48",
    "bn.js": "^5.1.1",
    "bip39": "^3.0.2",
    "chalk": "^4.0.0",
    "figlet": "^1.4.0",
    "fs-extra": "^9.0.0",
    "prompts": "^2.3.2",
    "express": "^4.17.1",
    "node-schedule": "^1.3.2",
    "snarkjs": "^0.1.25",
    "web3": "^1.2.6",
    "web3-core": "^1.2.6",
    "web3-core-subscriptions": "^1.2.6",
    "web3-utils": "^1.2.6",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "node-docker-api": "^1.1.22",
    "fs-extra": "^9.0.0"
  }
}
