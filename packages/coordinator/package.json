{
  "name": "@zkopru/coordinator",
  "version": "0.0.1",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "~account": "../account/dist",
    "~coordinator": "dist",
    "~core": "../core/dist",
    "~database": "../database/dist",
    "~testnet": "../testnet/dist"
  },
  "scripts": {
    "postinstall": "shx mkdir -p dist",
    "build": "tsc --build tsconfig.build.json && link-module-alias",
    "clean": "tsc --build tsconfig.build.json --clean && shx rm -rf coverage *.log junit.xml dist && jest --clearCache",
    "pretest": "make -C ../../ contract-container",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --reporters='jest-junit'",
    "coverage": "jest --coverage",
    "coverage:show": "live-server coverage",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@nano-sql/core": "^2.3.7",
    "@zkopru/babyjubjub": "file:../babyjubjub",
    "@zkopru/contracts": "file:../contracts",
    "@zkopru/core": "file:../core",
    "@zkopru/transaction": "file:../transaction",
    "@zkopru/utils": "file:../utils",
    "big-integer": "^1.6.48",
    "bn.js": "^5.1.1",
    "express": "^4.17.1",
    "node-schedule": "^1.3.2",
    "snarkjs": "^0.1.25",
    "web3": "^1.2.6",
    "web3-core": "^1.2.6",
    "web3-core-subscriptions": "^1.2.6",
    "web3-utils": "^1.2.6"
  },
  "devDependencies": {
    "node-docker-api": "^1.1.22"
  }
}
